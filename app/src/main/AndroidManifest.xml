<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andvilouette.vilouette">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"

        android:theme="@style/Theme.Vilouette">
        <activity android:name=".activity.Forgotpassword_act"></activity>
        <activity android:name=".activity.Signup_Act" />
        <activity
            android:name=".activity.Homenav_act"
            android:label="@string/title_activity_homenav_act"
            android:theme="@style/Theme.Vilouette.NoActionBar" />
        <activity android:name=".activity.Home_act" />
        <activity android:name=".activity.Login_act" />
        <activity android:name=".activity.Splash2_act" />
        <activity android:name=".activity.Splash_act">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>